<Window x:Class="KillMeHospitalManege.Poliklinik"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         Title="Poliklinik" Height="200.082" Width="358.784">
    <Grid>
        <TabControl HorizontalAlignment="Left" Height="159" Margin="0,10,-12,0" VerticalAlignment="Top" Width="363">
            <TabItem x:Name="TabYeniKayit" Header="Yeni Poliklinik Kaydı">
                <Grid Background="#FFE5E5E5" Margin="0,0,2,0" Height="134" VerticalAlignment="Top">
                    <Label Content="Poliklinik Adı : " HorizontalAlignment="Left" Margin="10,13,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="TBAdi" HorizontalAlignment="Left" Height="23" Margin="133,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="202"/>
                    <Button x:Name="Kayit" Content="Kaydet" HorizontalAlignment="Left" Margin="243,82,0,0" VerticalAlignment="Top" Width="75" Click="Kayit_Click"/>
                    <TextBlock HorizontalAlignment="Left" Margin="15,48,0,0" TextWrapping="Wrap" Text="Poliklinik Doktoru :" VerticalAlignment="Top"/>
                    <TextBox x:Name="TBDoktoru" HorizontalAlignment="Left" Height="23" Margin="133,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="202"/>
                </Grid>
            </TabItem>

        </TabControl>

    </Grid>
</Window>
