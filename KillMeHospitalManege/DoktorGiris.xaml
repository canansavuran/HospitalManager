<Window x:Class="KillMeHospitalManege.DoktorGiris"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Doktor Girisi" Height="200" Width="300">
    <Grid Background="#FFE5E5E5">
        <TextBlock HorizontalAlignment="Left" Margin="10,31,0,0" TextWrapping="Wrap" Text="Poliklinik :" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,74,0,0" TextWrapping="Wrap" Text="Doktor :" VerticalAlignment="Top"/>
        <Button x:Name="Kayit" Content="Giriş" HorizontalAlignment="Left" Margin="156,123,0,0" VerticalAlignment="Top" Width="112" Click="Kayit_Click"/>
        <ComboBox x:Name="CBPoliklinik" HorizontalAlignment="Left" Margin="94,25,0,0" VerticalAlignment="Top" Width="174" SelectionChanged="CBPoliklinik_SelectionChanged"/>
        <ComboBox x:Name="CBDoktor" HorizontalAlignment="Left" Margin="94,68,0,0" VerticalAlignment="Top" Width="174" IsEnabled="False" SelectedIndex="0"/>

    </Grid>
</Window>
